<Bindings>
	<Binding name="SMASH_TARGETMORE" category="BINDING_CATEGORY_SMASH">
		Smash_ToggleTargetMode()
	</Binding>
	<Binding name="SMASH_TARGETLESS" category="BINDING_CATEGORY_SMASH">
		Smash_ToggleTargetModeReverse()
	</Binding>
	<Binding name="SMASH_TARGET1" category="BINDING_CATEGORY_SMASH">
		Smash_SetTargetMode(1)
	</Binding>
	<Binding name="SMASH_TARGET2" category="BINDING_CATEGORY_SMASH">
		Smash_SetTargetMode(2)
	</Binding>
	<Binding name="SMASH_TARGET3" category="BINDING_CATEGORY_SMASH">
		Smash_SetTargetMode(3)
	</Binding>
	<Binding name="SMASH_TARGET4" category="BINDING_CATEGORY_SMASH">
		Smash_SetTargetMode(4)
	</Binding>
	<Binding name="SMASH_TARGET5" category="BINDING_CATEGORY_SMASH">
		Smash_SetTargetMode(5)
	</Binding>
</Bindings>
